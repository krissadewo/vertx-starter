# EditorConfig is awesome: https://EditorConfig.org
root = true

[*.java]
charset = utf-8
indent_style = space
indent_size = 4
trim_trailing_whitespace = true
end_of_line = lf
insert_final_newline = true

# Ignore paths
[{*.properties,*.yaml}]
charset = unset
end_of_line = unset
insert_final_newline = unset
trim_trailing_whitespace = unset
indent_style = unset
indent_size = unset
